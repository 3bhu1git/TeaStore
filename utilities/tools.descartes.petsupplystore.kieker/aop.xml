<!DOCTYPE aspectj PUBLIC "-//AspectJ//DTD//EN" "http://www.eclipse.org/aspectj/dtd/aspectj.dtd">
<aspectj>
	<weaver options="">
		<include within="tools.descartes.petsupplystore..*"/>
		<exclude within="tools.descartes.petsupplystore.entities..*" />
		<exclude within="tools.descartes.petsupplystore.rest..*" />
		<exclude within="tools.descartes.petsupplystore.registryclient.loadbalancers..*" />
		<exclude within="tools.descartes.petsupplystore.registryclient.RegistryClient" />
		<exclude within="tools.descartes.petsupplystore.registryclient.StartupCallbackTask" />
		<exclude within="tools.descartes.petsupplystore.registryclient.RegistryClientHeartbeatDaemon" />
		<exclude within="tools.descartes.petsupplystore.registryclient.Service" />
		<exclude within="tools.descartes.petsupplystore.registryclient.StartupCallback" />
		<exclude within="tools.descartes.petsupplystore.registryclient.rest.RestUtil" />
	</weaver>
	<aspects>
		<concrete-aspect name="kieker.monitoring.probe.aspectj.operationExecution.TargetedAspect" extends="kieker.monitoring.probe.aspectj.operationExecution.AbstractOperationExecutionAspect">
			<pointcut name="monitoredOperation" expression="execution(* *(..)) &amp;&amp; !execution(void *..lambda*(..))" />
		</concrete-aspect>
	</aspects>
</aspectj>
